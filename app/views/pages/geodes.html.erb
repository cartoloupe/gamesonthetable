<script src="//code.jquery.com/ui/1.11.4/jquery-ui.js"></script>
<div id='user_info' ng-controller='UserController'>

  <br>
  <div class="row">

    <div class="col-md-3 pull-left">
      <button ng-click='getUser()'>Refresh</button>
      <h3>At this table:</h3>
      <ul class="">
        <li ng-repeat="user in current_users">
          <span>{{user}}</span>
        </li>
      </ul>

      <div ng-controller='MovesController'>

        <div style="border-style:solid; width: 300px">
          <div class="row">
            <div class="col-md-4 resultsection">
              <ul>
                <li>Y</li>
                <li>N</li>
              </ul>
            </div>
            <div class="col-md-4 querysection">
              <ul>
                <li>guess</li>
                <li>ask</li>
              </ul>
            </div>
          </div>

          <div class="row">
            <div class="col-md-10 dotsection">
            </div>
          </div>

          <div class="row">
            <div class="col-md-8 controls">
              <svg height='100' width='250'>
                <circle class="items add-circle" cx=20 cy=50 fill='black' r='20' style="stroke:white"></circle>
                <polygon class="items add-triangle" points="{{trianglePoints(65,55,20)}}" style="fill:black;stroke:white;stroke-width:1" />
                <polygon class="items add-square" points="{{squarePoints(110,50,20)}}" style="fill:black;stroke:white;stroke-width:1" />
              </svg>
            </div>

            <div class="col-md-4 pull-right usersection">
              <p>{{current_user}}</p>
            </div>
          </div>

          <div class="row">
            <div class="col-md-4 chatsection">
              <input />
            </div>
            <div class="col-md-offset-6 col-md-2 pull-right deletesection">
              <button>X</button>
            </div>
          </div>
        </div>





      </div>


    </div>
  </div>

</div>
