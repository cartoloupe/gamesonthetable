<script src="//code.jquery.com/ui/1.11.4/jquery-ui.js"></script>
<div id='user_info' ng-controller='UserController'>

  <br>
  <div class="row">

    <div class="col-md-3 pull-left">
      <button ng-click='getUser()'>Refresh</button>
      <h3>At this table:</h3>
      <ul class="">
        <li ng-repeat="user in current_users">
          <span>{{user}}</span>
        </li>
      </ul>

      <div ng-controller='MovesController'>
        <div class="dotsection">
        </div>

        <div class="controls">
          <svg height='100' width='250'>
            <circle class="items add-circle" cx=20 cy=50 fill='black' r='20' style="stroke:white"></circle>
            <polygon class="items add-triangle" points="45,65 60,35 75,65" style="fill:black;stroke:white;stroke-width:1" />
            <rect class="items add-rect" x=90 y=30 width=40 height=40 style="fill:black;stroke-width:1;stroke:white" />
          </svg>
        </div>
      </div>


    </div>
  </div>

</div>
